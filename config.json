{
  "env": {
    "R": 8,
    "C": 4,
    "N": 6
  },
  "train": {
    "log_wandb": true,
    "l2_weight_reg": 0.0001,
    "batch_size": 512,
    "learning_rate": 0.001,
    "scheduler_step_size_in_batches": 1e5,
    "scheduler_gamma": 0.5,
    "min_lr": 1e-5,
    "buffer_size": 450000,
    "value_scaling": 0.1
  },
  "inference": {
    "can_only_add": true,
    "n_processes": 60,
    "batch_size": 25
  },
  "eval": {
    "testset_size": 15,
    "batch_interval": 6000
  },
  "mcts": {
    "c_puct": 5,
    "temperature": 0.6,
    "search_iterations": 100,
    "dirichlet_weight": 0.25,
    "dirichlet_alpha": 1
  },
  "nn": {
    "blocks": 16,
    "hidden_channels": 32,
    "hidden_kernel_size": 3,
    "hidden_stride": 1,
    "policy_channels": 2,
    "policy_kernel_size": 1,
    "policy_stride": 1,
    "value_channels": 1,
    "value_kernel_size": 1,
    "value_stride": 1,
    "value_hidden": 256
  }
}
